generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Doador{
  id          Int        @id @default(autoincrement())
  createdAt   DateTime   @default(now())
  fullName    String
  CPF         String 
  mail        String
  password    String
}

model Coletor{
  id            Int       @id @default(autoincrement())
  createdAt     DateTime  @default(now())
  BussinesName  String
  CNPJ          String 
  mail          String
  password      String
}